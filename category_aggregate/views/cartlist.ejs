<table border="1">
    <tr>
        <th>Category Name</th>
        <th>Product Name</th>
        <th>Product Price</th>
        <th>Quantity</th>
        <th>Gross-Amount</th>
    </tr>
    <% allData.map(data => { %>
        <tr>
            <td><%= data.product.category.name %></td>
            <td><%= data.product.name %></td>
            <td><%= data.product.price %></td> 
            <td><%= data.quantity %></td>
            <td><%= data.grossamount %></td>
        </tr>
    <% }) %>
</table>
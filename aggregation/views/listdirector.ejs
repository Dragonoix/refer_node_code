<table border="1">
    <tr>
        <th>Sl. No.</th>
        <th>Name</th>
        <th>origin</th>
    </tr>
    <% allData.map((data, index) => { %>
        <tr>
            <td><%= ++index %></td>
            <td><%= data.name %></td>
            <td><%= data.origin %></td>
        </tr>
    <% }) %>
</table>
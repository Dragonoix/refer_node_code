<form action="/singerinsert" method="post">
    <label>Name</label>
    <input type="text" name="name" id="name" required> <br>
    <select name="movie" id="movie">
        <% allmovie.map(data => { %>
            <option value="<%= data._id %>"><%= data.name %></option>
        <% }) %>
    </select>
    <input type="submit" name="submit">
</form>